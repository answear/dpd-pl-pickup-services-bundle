{
    "name": "answear/dpd-pl-pickup-services-bundle",
    "description": "DPD.pl Pickup Services integration for Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-simplexml": "*",
        "ext-xmlreader": "*",
        "guzzlehttp/guzzle": "^7.8.2",
        "guzzlehttp/psr7": "^2.6",
        "symfony/http-kernel": "^7.0.3",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpro/grumphp": "^2.12",
        "phpunit/phpunit": "^10.5.27",
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^7.0.3"
    },
    "autoload": {
        "psr-4": {
            "Answear\\DpdPlPickupServicesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Answear\\DpdPlPickupServicesBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "GrumPHP\\Composer\\DevelopmentIntegrator::integrate"
        ],
        "post-update-cmd": [
            "GrumPHP\\Composer\\DevelopmentIntegrator::integrate"
        ]
    },
    "extra": {
        "grumphp": {
            "config-default-path": "grumphp.yaml"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
